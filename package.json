{
  "name": "nengo",
  "version": "0.2.1",
  "description": "Frontend components for Nengo GUI",
  "author": "Nengo developers",
  "license": "Free for non-commercial use",
  "main": "nengo_gui/static/dist/nengo.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nengo/nengo_gui.git"
  },
  "bugs": {
    "url": "https://github.com/nengo/nengo_gui/issues"
  },
  "private": true,
  "scripts": {
    "build": "webpack --debug --devtool source-map --output-pathinfo --no-docs",
    "debug": "webpack --config webpack.debug.js --no-docs && http-server nengo_gui/static/dist",
    "release": "webpack --optimize-minimize --optimize-occurence-order",
    "style": "tslint 'nengo_gui/**/*.ts' && stylelint 'nengo_gui/**/*.css'",
    "test": "ts-node node_modules/tape/bin/tape 'nengo_gui/**/tests/*.test.ts' | faucet",
    "postinstall": "typings install"
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "devDependencies": {
    "bootstrap": "^3.3.4",
    "bootstrap-validator": "^0.8.1",
    "brace": "^0.8.0",
    "css-loader": "^0.23.1",
    "d3": "^3.5.3",
    "expose-loader": "^0.7.1",
    "faucet": "0.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "http-server": "^0.9.0",
    "imports-loader": "^0.6.5",
    "interact.js": "^1.2.6",
    "jquery": "^2.1.3",
    "jquery-ui": "^1.10.4",
    "jqueryfiletree": "jqueryfiletree/jqueryfiletree#2.1.4",
    "jsdom": "^9.5.0",
    "maquette": "^2.3.5",
    "style-loader": "^0.13.1",
    "stylelint": "^7.3.1",
    "tape": "^4.6.0",
    "ts-loader": "^0.8.2",
    "ts-node": "^1.3.0",
    "tslint": "^3.14.0",
    "tslint-eslint-rules": "^1.3.0",
    "tslint-microsoft-contrib": "^2.0.10",
    "typedoc": "^0.4.5",
    "typedoc-webpack-plugin": "^1.0.9",
    "typescript": "^1.8.10",
    "typings": "^1.3.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-fail-plugin": "^1.0.5"
  }
}
